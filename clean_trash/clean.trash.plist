<?xml version=”1.0" encoding=”UTF-8"?>
<!DOCTYPE plist PUBLIC “-//Apple//DTD PLIST 1.0//EN” “http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version=”1.0">
<dict>
  <key>Label</key>
  <string>local.clean.trash</string>
 
  <key>Program</key>
  <string>/absolute/path/to/executable/clean.trash</string>

  <key>StartCalendarInterval</key>
  <array>
    <dict>
      <key>Weekday</key>
      <integer>7</integer>
      <key>Hour</key>
      <integer>18</integer>
      <key>Minute</key>
      <integer>16</integer>
    </dict>
  </array>

  <key>StandardErrorPath</key>
  <string>/tmp/local.clean.trash.err</string>
  <key>StandardOutPath</key>
  <string>/tmp/local.clean.trash.out</string>
</dict>
</plist>