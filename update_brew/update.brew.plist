<?xml version=”1.0" encoding=”UTF-8"?>
<!DOCTYPE plist PUBLIC “-//Apple//DTD PLIST 1.0//EN” “http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version=”1.0">
<dict>
  <key>Label</key>
  <string>local.update.brew</string>
 
  <key>Program</key>
  <string>/absolute/path/to/executable/update.brew</string>

  <key>StartCalendarInterval</key>
  <array>
    <dict>
      <key>Hour</key>
      <integer>18</integer>
      <key>Minute</key>
      <integer>06</integer>
    </dict>
  </array>

  <key>StandardErrorPath</key>
  <string>/tmp/local.update.brew.err</string>
  <key>StandardOutPath</key>
  <string>/tmp/local.update.brew.out</string>
</dict>
</plist>